<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="com.HashHub.UI.Usuarios.Index.IndexController"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Label GridPane.rowIndex="2" GridPane.columnIndex="2">Bienvenido a HashHub Music !</Label>

    <Button text="Iniciar sesion" GridPane.rowIndex="4" GridPane.columnIndex="1" onAction="#redirigirInicioSesion"/>

    <Button text="Registrarse" GridPane.rowIndex="4" GridPane.columnIndex="3" onAction="#redirigirRegistrarse"/>
</GridPane>